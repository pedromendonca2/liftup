    "@testing-library/react-native": "13.3.3",
    "@types/jest": "29.5.12",
    "@types/react": "19.0.10",
    "@types/react-test-renderer": "19.0.0",
    "babel-jest": "30.1.1",
    "babel-preset-expo": "13.2.3",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
